{
    "json.schemas": [
        {
            "fileMatch": [
                "**/release-please-config.json"
            ],
            "url": "https://raw.githubusercontent.com/googleapis/release-please/refs/heads/main/schemas/config.json"
        },
        {
            "fileMatch": [
                "**/.release-please-manifest.json"
            ],
            "url": "https://raw.githubusercontent.com/googleapis/release-please/refs/heads/main/schemas/manifest.json"
        }
    ],
    "yaml.schemas": {
        "https://json.schemastore.org/github-issue-config.json": ".github/ISSUE_TEMPLATE/config.yml"
    },
    "workbench.colorCustomizations": {
        "[Default Dark+]": {
            "editorInlayHint.foreground": "#868686f0",
            "editorInlayHint.background": "#3d3d3d48",
            "editorInlayHint.typeForeground": "#fdb6fdf0",
            "editorInlayHint.parameterForeground": "#fdb6fdf0"
        }
    },
    "editor.semanticTokenColorCustomizations": {
        "[Default Dark+]": {
            "rules": {
                "operator.unsafe": "#ff6600",
                "function.unsafe": "#ff6600",
                "method.unsafe": "#ff6600"
            }
        }
    },
    "rust-analyzer.cargo.buildScripts.enable": true
}
